{
    "family": "node-js-app-task",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:%BUILD_NUMBER%",
            "name": "nodejs-build",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "networkMode": "awsvpc",
            "entryPoint": [],
            requires_compatibilities = ["FARGATE"],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
