{
    "family": "node-js-app-task",
     "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
   },
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:%BUILD_NUMBER%",
            "name": "nodejs-build",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
